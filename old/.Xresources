!---------------
! ROFI Color theme
!---------------
rofi.color-enabled: true
rofi.color-window: #2f343f, #2f343f, #2f343f
rofi.color-normal: #2f343f, #f3f4f5, #2f343f, #586e75, #f3f4f5
rofi.color-active: #2f343f, #268bd2, #2f343f, #268bd2, #fdf6e3
rofi.color-urgent: #2f343f, #dc322f, #2f343f, #dc322f, #fdf6e3

!---------------
! URxvt
!---------------

Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull

URxvt.letterSpace: -1
URxvt.font: xft:Menlo:size=10
URxvt.scrollBar: false
